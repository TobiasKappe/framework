{
	"name": "mako/framework",
	"description": "Mako Framework",
	"keywords": ["mako", "framework"],
	"homepage": "http://makoframework.com",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Frederic G. Ã˜stby",
			"email": "frederic.g.ostby@gmail.com"
		},
		{
			"name": "Contributors",
			"homepage": "https://makoframework.com/contributors"
		}
	],
	"support": {
		"issues": "https://github.com/mako-framework/framework/issues",
		"source": "https://github.com/mako-framework/framework"
	},
	"autoload": {
		"psr-4": {
			"mako\\": "src/mako"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"mako\\tests\\": "tests"
		}
	},
	"require": {
		"php": ">=7.0.0",
		"monolog/monolog": "^1.22.0",
		"filp/whoops": "^2.1",
		"symfony/var-dumper": "^3.4 || ^4.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.5",
		"mockery/mockery": "^1.0"
	},
	"prefer-stable": true,
	"minimum-stability": "dev",
	"extra": {
		"branch-alias": {
			 "dev-master": "6.0-dev"
		}
	}
}
